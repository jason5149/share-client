{
  "presets": [
    [
      "@babel/preset-env",
      { "modules": false }
    ]
  ],
  "plugins": [
    "@babel/plugin-syntax-dynamic-import",
    "@babel/plugin-transform-runtime",
    [
      "component",
      {
        "libraryName": "mint-ui",
        "style": true
      },
      "mint-ui"
    ],
    [
      "import",
      { 
        "libraryName": "pms-saas-common", 
        "camel2DashComponentName": false
      },
      "pms-saas-common"
    ]
  ]
}